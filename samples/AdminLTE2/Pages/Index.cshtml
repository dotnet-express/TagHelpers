@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        Blank page
        <small>it all starts here</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="#">Examples</a></li>
        <li class="active">Blank page</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">
    <!-- Default box -->
    <div class="box">
        <div class="box-header with-border">
            <h3 class="box-title">Title</h3>

            <div class="box-tools pull-right">
                <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip"
                        title="Collapse">
                    <i class="fa fa-minus"></i>
                </button>
                <button type="button" class="btn btn-box-tool" data-widget="remove" data-toggle="tooltip" title="Remove">
                    <i class="fa fa-times"></i>
                </button>
            </div>
        </div>
        <div class="box-body">
            Start creating your amazing application!
            <style>
                .container .row div {
                    border: 1px solid black;  
                    padding: 5px 5px 5px 5px;
                }
                .container-fluid .row div {
                    border: 1px solid black;
                    padding: 5px 5px 5px 5px;
                }
            </style>
            <div class="container">
                <div class="row">
                    <div class="col-md-1">.col-md-1</div>
                    <div class="col-md-1">.col-md-1</div>
                    <div class="col-md-1">.col-md-1</div>
                    <div class="col-md-1">.col-md-1</div>
                    <div class="col-md-1">.col-md-1</div>
                    <div class="col-md-1">.col-md-1</div>
                    <div class="col-md-1">.col-md-1</div>
                    <div class="col-md-1">.col-md-1</div>
                    <div class="col-md-1">.col-md-1</div>
                    <div class="col-md-1">.col-md-1</div>
                    <div class="col-md-1">.col-md-1</div>
                    <div class="col-md-1">.col-md-1</div>
                </div>
            </div>
            <container-fluid>
                <row>
                    <column size-md="1">.col-md-1</column>
                    <column size-md="1">.col-md-1</column>
                    <column size-md="1">.col-md-1</column>
                    <column size-md="1">.col-md-1</column>
                    <column size-md="1">.col-md-1</column>
                    <column size-md="1">.col-md-1</column>
                    <column size-md="1">.col-md-1</column>
                    <column size-md="1">.col-md-1</column>
                    <column size-md="1">.col-md-1</column>
                    <column size-md="1">.col-md-1</column>
                    <column size-md="1">.col-md-1</column>
                    <column size-md="1">.col-md-1</column>
                </row>
                <row>
                    <column size-md="8">.col-md-8</column>
                    <column size-md="4">.col-md-4</column>
                </row>
                <row>
                    <column size-md="4">.col-md-4</column>
                    <column size-md="4">.col-md-4</column>
                    <column size-md="4">.col-md-4</column>
                </row>
                <row>
                    <column size-md="6">.col-md-6</column>
                    <column size-md="6">.col-md-6</column>
                </row>
                <row>
                    <column size-md="4">.col-md-4</column>
                    <column size-md="4" offset-md="4">.col-md-4 .col-md-offset-4</column>
                </row>
                <row>
                    <column size-md="3" offset-md="3">.col-md-3 .col-md-offset-3</column>
                    <column size-md="3" offset-md="3">.col-md-3 .col-md-offset-3</column>
                </row>
                <row>
                    <column size-md="6" offset-md="3">.col-md-6 .col-md-offset-3</column>
                </row>
            </container-fluid>
        </div>
        <!-- /.box-body -->
        <div class="box-footer">
            Footer
        </div>
        <!-- /.box-footer-->
    </div>
<!-- /.box -->

</section>
<!-- /.content -->