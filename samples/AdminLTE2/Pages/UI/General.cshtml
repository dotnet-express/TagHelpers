    <!-- START ALERTS AND CALLOUTS -->
    <page-header>Alerts and Callouts</page-header>

    <row>
        <column size-md="6">
            <box>
                <box-header fa-icon="warning">
                    <box-title>Alerts</box-title>
                </box-header>
                <!-- /.box-header -->
                <box-body>
                    <alert-danger dismissible="true">
                        <h4><i class="icon fa fa-ban"></i> Alert!</h4>
                        Danger alert preview. This alert is dismissable. A wonderful serenity has taken possession of my entire
                        soul, like these sweet mornings of spring which I enjoy with my whole heart.<br />
                        <a href="#">Change a few things up</a> and try submitting again.
                    </alert-danger>

                    <alert-info dismissible="true">
                        <h4><i class="icon fa fa-info"></i> Alert!</h4>
                        Info alert preview. This alert is dismissable.<br />
                        <a href="#">Change a few things up</a> and try submitting again.
                    </alert-info>

                    <alert-warning dismissible="true">
                        <h4><i class="icon fa fa-warning"></i> Alert!</h4> 
                        <a href="#" class="text-uppercase">Change a few things up</a> and try submitting again.
                    </alert-warning>

                    <alert-success dismissible="true">
                        <h4><i class="icon fa fa-check"></i> Alert!</h4>
                        Success alert preview. This alert is dismissable.<br />
                        <a href="#">Change a few things up</a> and try submitting again.
                    </alert-success>
                </box-body>
                <!-- /.box-body -->
            </box>
            <!-- /.box -->
        </column>
        <!-- /.col -->

        <column size-md="6">
            <box>
                <box-header fa-icon="bullhorn">
                    <box-title>Callouts</box-title>
                </box-header>
                <!-- /.box-header -->
                <box-body>
                    <callout-danger>
                        <h4>I am a danger callout!</h4>

                        <p>
                            There is a <a href="#">problem that we need to fix</a>. A wonderful serenity has taken possession of my entire soul,
                            like these sweet mornings of spring which I enjoy with my whole heart.
                        </p>
                    </callout-danger>
                    <callout-info>
                        <h4>I am an info callout!</h4>

