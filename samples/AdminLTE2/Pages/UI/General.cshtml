    <page-header>Progress Bars</page-header>

    <row>
        <column size-md="6">
            <box solid="true">
                <box-header with-border="true">
                    <box-title>Progress Bars Different Sizes</box-title>
                </box-header>
                <!-- /.box-header -->
                <box-body>
                    <p><code>.progress</code></p>

                    <progress-bar value="40" color="Primary" striped="true" labeled="true">
                        Complete (success)
                    </progress-bar>

                    <p>Class: <code>.sm</code></p>

                    <progress-bar value="20" color="Success" striped="true" animated="true" height="10">
                        <sr-only>20% Complete</sr-only>
                    </progress-bar>

                    <p>Class: <code>.xs</code></p>

                    <progress-bar value="60" color="Warning" striped="true" height="7">
                        <sr-only>60% Complete (warning)</sr-only>
                    </progress-bar>

                    <p>Class: <code>.xxs</code></p>

                    <progress-bar value="60" color="Danger" striped="true" height="3">
                        <sr-only>60% Complete (danger)</sr-only>
                    </progress-bar>

                </box-body>
                <!-- /.box-body -->
            </box>
            <!-- /.box -->
        </column>
        <!-- /.col (left) -->
        <column size-md="6">
